#include "stdafx.h"
#include "iostream"
#include <math.h>

using namespace std;

int main() {

	double a, b, n, m, N;
	a = 4.8; b = -7.9;
	N = a - b;
	cout << "a = " << a << endl;
	cout << "b = " << b << endl;

	if (a >= b)
	{
		n = pow(N, 1. / 3);
	}
	else if (a<b)
	{
		n = pow(a, 2) + (N / sinl(a*b));
	}
	if (n<b)
	{
		m = ((n - a) / (-b)) + pow((pow((sinl(a)), 2) - cosl(n)), 1. / 2);
	}
	else if (n == b)
	{
		m = pow(b, 2) + tan(n*a);
	}
	else if (n>b)
	{
		m = pow(b, 3) + n*pow(a, 2);
	}
	cout << "n = " << n << endl;
	cout << "m = " << m << endl;

	system("pause");
	return 0;
}


