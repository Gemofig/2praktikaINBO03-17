#include "stdafx.h"
#include "iostream"
#include <math.h>

using namespace std;
int main()
{
	double a, b, z, t;
	a = -0.6; b = 5.3;
	cout << "a = " << a << endl;
	cout << "b = " << b << endl;

	if (a<b)
	{
		z = pow(sqrt(pow((pow(a, 2) - pow(b, 2)), 2)), 1. / 2);
	}
	else if (a >= b)
	{
		z = 1 - 2 * cosl(a)*sinl(b);
	}
	if (z<b)
	{
		t = pow((z + pow(a, 2)*b), 1. / 3);
	}
	else if (z == b)
	{
		t = 1 - log10(z) + cosl(pow(a, 2)*b);
	}
	else if (z>b)
	{
		t = 1 / cosl(z*a);
	}

	cout << "z = " << z << endl;
	cout << "t = " << t << endl;

	system("pause");
	return 0;
}


